# Crossplane config for Galley
#

. $path/etc/defaults

# A unique host name is not required
host=crossplane
size=c-2

# Build command--spaces, pipes, semicolons and other special characters *must*
# be escaped and double quoted if used
cmd="curl\ https://raw.githubusercontent.com/fuzz/crossplane-setup/main/crossplane-setup.sh\ \|\ sh"
